<p id="notice"><%= notice %></p>

<h1>Listing Batches</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Package</th>
      <th>Test type</th>
      <th>Component scores</th>
      <th>Instructor</th>
      <th>Number of students</th>
      <th>Essay file</th>
      <th>Complete</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @batches.each do |batch| %>
      <tr>
        <td><%= batch.company %></td>
        <td><%= batch.package %></td>
        <td><%= batch.test_type %></td>
        <td><%= batch.component_scores %></td>
        <td><%= batch.instructor %></td>
        <td><%= batch.number_of_students %></td>
        <td><%= batch.essay_file %></td>
        <td><%= batch.complete %></td>
        <td><%= link_to 'Show', batch %></td>
        <td><%= link_to 'Edit', edit_batch_path(batch) %></td>
        <td><%= link_to 'Destroy', batch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Batch', new_batch_path %>
